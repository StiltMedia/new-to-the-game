<section id="page-header" class="background41 parallax big-header">
  <div class="page_header_inner clearfix white oswald uppercase normal antialiased">
    <div class="left f-left">
      <h2 class="page_header">
        Contact
      </h2>
      <h5 class="page_note">
        Want more INFO?
      </h5>
    </div>
  </div>
</section>

<section id="contact" class="relative contact">
  <div class="inner t-center clearfix animated" data-animation="fadeIn" data-animation-delay="800">
    <h1 class="header header-style-2 dark oswald strip uppercase t-center f30">
      Are you an athletic service provider who would like to join our network?<br><span class="red">Do you have questions or comments?</span><br>Want more into?
    </h1>
    <p class="light t-center">
      Drop Us a Message
    </p>

    <%= render 'form' %>

  </div>
</section>

<section class="address type-2 background37 parallax9 dark-bg">
  <div class="boxed clearfix relative">
    <a class="box clearfix block f-left" href="tel:7866109065">
      <div class="f-left icon">
        <i class="fa fa-mobile"></i>
      </div>
      <div class="f-right texts">
        <span class="arrow"></span>
        <h3 class="no-padding no-margin extra-light">
          Phone
        </h3>
        <p class="no-margin no-padding light">
          786 610 9065
        </p>
      </div>
    </a>
    <a class="box clearfix block f-left" href="mailto:contactus@newtothegame.org">
      <div class="f-left icon">
        <i class="fa fa-envelope"></i>
      </div>
      <div class="f-right texts">
        <span class="arrow"></span>
        <h3 class="no-padding no-margin extra-light">
          E-Mail
        </h3>
        <p class="no-margin no-padding extra-light">
          contactus@newtothegame.org
        </p>
      </div>
    </a>
    <a class="box clearfix block f-left scroll" href="#map">
      <div class="f-left icon">
        <i class="fa fa-map-marker"></i>
      </div>
      <div class="f-right texts">
        <span class="arrow"></span>
        <h3 class="no-padding no-margin extra-light">
          Address
        </h3>
        <p class="no-margin no-padding extra-light">
          New To The Game, Inc.<br>
          P.O. Box 924423<br>
          Homestead, Fl 33092
        </p>
      </div>
    </a>
  </div>
