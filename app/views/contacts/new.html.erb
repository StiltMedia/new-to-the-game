    <h1 class="header header-style-1 white normal v-center uppercase antialiased oswald t-center">
      new to the game, inc.
    </h1>
  </div>
  <!-- Ful Screen Slider -->
  <div id="video_section" class="fullscreen background34 absolute video xdark-bg">
    <!-- Video -->
    <div id="p1" class="player video-container" data-property="{videoURL:'G2AvRfxgpL4', containment:'#video_section', autoPlay:true, showControls:true, mute:true, startAt:0, opacity:1}"></div>
    <!-- End Video -->
  </div>
</section>
